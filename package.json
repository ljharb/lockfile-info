{
	"name": "lockfile-info",
	"version": "0.0.0",
	"description": "Info about an npm project - which lockfile version, which lockfile(s) are present, etc.",
	"main": "index.js",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"prepublishOnly": "safe-publish-latest",
		"lint": "eslint --ext=js,mjs .",
		"pretest": "npm run lint",
		"tests-only": "tape 'test/**/*.js'",
		"test": "npm run tests-only",
		"posttest": "aud --production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/lockfile-info.git"
	},
	"keywords": [
		"npm",
		"lockfile",
		"package-lock",
		"shrinkwrap"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/lockfile-info/issues"
	},
	"homepage": "https://github.com/ljharb/lockfile-info#readme",
	"devDependencies": {
		"@ljharb/eslint-config": "^19.1.0",
		"aud": "^1.1.5",
		"eslint": "^8.4.0",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.3.2"
	},
	"engines": {
		"node": ">= 8"
	},
	"dependencies": {
		"call-bind": "^1.0.2"
	}
}
